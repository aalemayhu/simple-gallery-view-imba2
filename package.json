{
  "name": "simple-gallery-view-imba2",
  "version": "0.0.7",
  "description": "a very simple gallery view to experiment with imba 2",
  "repository": "git@github.com:scanf/simple-gallery-view-imba.git",
  "author": "Alexander Alemayhu <alexander@alemayhu.com>",
  "license": "MIT",
  "scripts": {
    "build": "imbac --standalone index.imba",
    "watch": "imbac -w --standalone index.imba",
    "check-version": "imbac --version",
    "production": "imbac -o dist --standalone index.imba",
    "release": "npm run production && npm version patch && npm publish"
  },
  "main": "dist/index.js",
  "keywords": [
    "imba2",
    "gallery",
    "simple"
  ],
  "dependencies": {
    "imba": "^2.0.0-alpha.10"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4"
  }
}
